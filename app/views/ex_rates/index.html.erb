<p id="notice"><%= notice %></p>

<h1>Ex Rates</h1>

<table>
  <thead>
    <tr>
      <th>M1</th>
      <th>M2</th>
      <th>M1c</th>
      <th>M2c</th>
      <th>Rate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ex_rates.each do |ex_rate| %>
      <tr>
        <td><%= ex_rate.m1 %></td>
        <td><%= ex_rate.m2 %></td>
        <td><%= ex_rate.m1c %></td>
        <td><%= ex_rate.m2c %></td>
        <td><%= ex_rate.rate %></td>
        <td><%= link_to 'Show', ex_rate %></td>
        <td><%= link_to 'Edit', edit_ex_rate_path(ex_rate) %></td>
        <td><%= link_to 'Destroy', ex_rate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ex Rate', new_ex_rate_path %>
